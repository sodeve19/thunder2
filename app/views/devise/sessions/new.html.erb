<h2>Log in</h2>


<!--form role="form" class="col-md-3"-->
  <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: 'col-md-3', role: 'form'}) do |f|%>
    <div><%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, :class => 'form-control col-md-3' %></div>

    <div><%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", :class => 'form-control' %></div>

    <% if devise_mapping.rememberable? -%>
      <div><%= f.check_box :remember_me %><%= f.label :remember_me %></div>
    <% end -%>

    <div><%= f.submit "Log in" ,  :class => "btn btn-default top"%></div>

    <%= render "devise/shared/links" %>
  <% end %>

<!--/form-->
